            <Label layoutX="77.0" layoutY="28.0" prefHeight="21.0" prefWidth="196.0" text="New Entry" textFill="WHITE">
               <font>
                  <Font size="23.0" />
               </font>
            </Label>
            <FontAwesomeIconView fill="#d665b5" glyphName="USER" layoutX="23.0" layoutY="108.0" size="14.0" />
            <FontAwesomeIconView fill="#d665b5" glyphName="LOCK" layoutX="24.0" layoutY="176.0" size="14.0" />
